##
# Expo public env vars (available in JS as process.env.EXPO_PUBLIC_*)
#
# Copy this file to `mobile/.env` (or set env vars in your shell) and restart Expo.
##

# Use Firebase emulators in development.
# Optional: if not set, defaults to enabled in dev (__DEV__), disabled in prod.
EXPO_PUBLIC_USE_EMULATORS=1

# When using emulators on a physical device: set this to the LAN IP of your PC
# (the same one shown by Expo as exp://<IP>:8081).
EXPO_PUBLIC_EMULATOR_HOST=192.168.X.Y

##
# Firebase Web config (Firebase Console -> Project settings -> Your apps -> Web app)
# These are not secrets, but we keep them in env so builds are reproducible (EAS).
##
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
# optional
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=
